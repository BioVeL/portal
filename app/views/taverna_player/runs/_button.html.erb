<div id="run-button">
  <% if run.complete? %>
    <%= link_to 'Delete', run, :method => :delete,
      :data => { confirm: 'Are you sure?' }, :class => "link_img_rem" %>
  <% else %>
    <%= link_to "Cancel", cancel_run_path(run.id), :method => :put,
      :data => { confirm: "Are you sure?" }, :class => "link_img_rem" %>
  <% end %>
</div>
